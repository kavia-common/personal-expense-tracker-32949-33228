# Copy to .env and fill with your environment values

# DATABASE
# For PostgreSQL with SQLAlchemy 2.x and psycopg
# Example: postgresql+psycopg://postgres:postgres@localhost:5432/expenses
DATABASE_URL=postgresql+psycopg://<USER>:<PASSWORD>@<HOST>:<PORT>/<DB_NAME>

# Security
SECRET_KEY=change-me-please
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS
# Comma-separated list, e.g. http://localhost:3000,https://myapp.com
CORS_ORIGINS=http://localhost:3000
